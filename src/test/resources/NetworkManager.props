#
# Global Defaults
#
ssl_keyStoreFile = my-keystore.jks
ssl_keyStorePassphrase=password
ssl_trustStoreFile = my-truststore.jks
ssl_trustStorePassphrase=password
ssl_keyManagerFactory = SunX509
ssl_trustManagerFactory = SunX509
ssl_SSLContext =TLS
ssl_keyStore =JKS
ssl_trustStore =JKS
ssl_clientCertificateRequired = false

#
# tcp layer configuration
#
receiveBufferSize = 128000
sendBufferSize = 128000
timeout = 60000
backlog = 100

readDelayOnFragmentation = 100
enableReadDelayOnFragmentation = true

#
# Generic protocol configuration
#
serverReadBufferSize =  1M
serverWriteBufferSize = 1M
selectorThreadCount   = 4

#
# MQTT Protocol configuration
#
# One Day, by default is the length of time a MQTT sessionImpl will be stored before it is deleted
#
maximumSessionExpiry = 86400
maximumBufferSize=10M

#
# MQTT 5.0 specific
#
minServerKeepAlive = 0
maxServerKeepAlive = 60
serverReceiveMaximum = 10
clientReceiveMaximum = 65535
clientMaximumTopicAlias = 32767
serverMaximumTopicAlias = 0


#
# Server interface 0
#
url_0 = tcp://0.0.0.0:8675/
protocol_0 = stomp,ws,mqtt
auth_0 = public

#
# Server interface 1
#
url_1 = tcp://0.0.0.0:2001/
protocol_1 = all
auth_1 = test

#
# Server interface 2
#
url_2 = ssl://0.0.0.0:8444/
protocol_2 = stomp,mqtt,all
auth_2 = public

#
# Server interface 3, Should not run since passphrase is wrong
#
url_3 = ssl://0.0.0.0:9999/
protocol_3 = stomp,mqtt,all
auth_3 = public
ssl_keyStorePassphrase_3=invalidPassword

#
# Server interface 4, SSL Client Authentication
#
url_4 = ssl://0.0.0.0:8445/
protocol_4 = stomp,mqtt,all,ws
auth_4 = ssl
ssl_clientCertificateRequired_4 = true


#
# Server interface 5
#
url_5 = tcp://0.0.0.0:1883/
protocol_5 = mqtt,ws
auth-5 = test

#
# UDP Server interface 6
#
url_6 = udp://0.0.0.0:1884/
protocol_6 = mqtt-sn
eventsPerTopicDuringSleep_6 = 10
maxTopicsInSleep_6 = 10

# ---------------------------------------------------------------------------------------------------------
# Serial Port Interface - GPS serial device
#  Device , Baud, Data Bits, Parity
#
url_7 = serial://com13,9600,8,N,1/
protocol_7 = nmea
auth_7 = anon
selectorThreadCount_7 = 1


# ---------------------------------------------------------------------------------------------------------
# Serial Port Interface - Lora MQTT-SN Gateway Arduino device
#
#  Device , Baud, Data Bits, Parity
#
url_8 = serial://com6,115200,8,N,1/?address=64&power=20&key=0x01,0x02,0x03,0x04,0x05,0x06,0x07,0x08,0x09,0x10,0x11,0x12,0x13,0x14,0x15,0x16
protocol_8 = Lora_MQTT-SN
auth_8 = anon
selectorThreadCount_8 = 1

#
# Server interface 1
#
url_9 = tcp://0.0.0.0:5672/
protocol_9 = amqp
auth_9 = public
SaslMechanisms_9 = ANONYMOUS, CRAM_MD5, DIGEST-MD5, GSSAPI
