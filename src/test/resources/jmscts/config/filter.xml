<?xml version="1.0"?>

<!-- ====================================================================== -->
<!-- Sample test case filter                                                -->
<!--                                                                        -->
<!-- This excludes tests using XAQueueConnectionFactory and                 -->
<!-- XATopicConnectionFactory which are not supported by OpenJMS 0.7.6      -->
<!-- ====================================================================== -->

<filter>
  <exclude>
    <factory>XAQueueConnectionFactory</factory>
  </exclude>

  <exclude>
    <factory>XATopicConnectionFactory</factory>
  </exclude>
</filter>
