#
# Time between checking the delay time of messages to be published
# In Milliseconds
#
# Default 1 second
DelayedPublishInterval = 1000

#
# The number of simultanous pipelines to use for Session creation and deletion
#
SessionPipeLines = 10

#
# Incomplete transaction life time that the server will hold onto the transaction before aborting it
# in Milliseconds
#
# Default 1 hour
TransactionExpiry = 3600000

#
# Transaction expiry scan. The interval between scans for expired transactins
# in Milliseconds
#
# Default 1 Second
TransactionScan = 1000